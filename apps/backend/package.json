{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "verify": "node scripts/verify-setup.js",
    "db:create": "node scripts/create-database.js",
    "db:migrate": "node scripts/run-migrations.js",
    "predb:seed": "npm --prefix ../.. run build --workspace=@nursequest/shared && npm --prefix ../.. run build --workspace=@nursequest/game-engine",
    "db:seed": "ts-node scripts/seed-database.ts",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "lint": "echo \"no lint\"",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/typeorm": "^10.0.2",
    "@nursequest/game-engine": "file:../../packages/game-engine",
    "@nursequest/shared": "file:../../packages/shared",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.6.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.19",
    "@types/passport-jwt": "^3.0.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
